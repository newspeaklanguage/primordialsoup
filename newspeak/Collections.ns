private capacityFor: numElements <Integer> = (
	numElements < 0 ifTrue: [^(ArgumentError value: numElements) signal].
	(* + 1 for an empty slot to terminate probing *)
	(* * 4 // 3 for 75% load factor *)
	(* << 1 for flattened association *)
	^numElements + 1 * 4 // 3 << 1
)
