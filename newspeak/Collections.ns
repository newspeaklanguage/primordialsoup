	oldTable:: table.
	table:: newTable:: Array new: oldTable size << 1.
	1 to: newTable size by: 2 do:
		[:newIndex | newTable at: newIndex put: newTable].
	1 to: oldTable size by: 2 do: [:oldIndex |
		| key = oldTable at: oldIndex. |
